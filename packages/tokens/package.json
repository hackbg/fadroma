{
  "name":    "@fadroma/tokens",
  "version": "7.0.0",
  "type":    "module",
  "main":    "tokens.ts",
  "files": [ "*.ts" ],
  "scripts": {
    "check": "tsc --noEmit",
    "test":  "true || ensuite tokens.spec.ts.md",
    "cov":   "ensuite-cov -r text -- tokens.spec.ts.md",
    "lcov":  "ensuite-cov -r lcov -- tokens.spec.ts.md",
    "ubik":  "npm run check && npm test && ubik"
  },
  "dependencies": {
    "@fadroma/core": "workspace:^2",
    "@fadroma/scrt": "workspace:^8",
    "@hackbg/4mat":  "workspace:^1",
    "@hackbg/logs":  "workspace:^1",
    "@hackbg/many":  "workspace:^1",
    "@hackbg/task":  "workspace:^1",
    "@hackbg/oops":  "workspace:^1"
  },
  "devDependencies": {
    "@hackbg/ensuite": "^1.1"
  }
}

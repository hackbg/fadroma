{
  "name":    "@fadroma/devnet",
  "version": "1.0.5",
  "type":    "module",
  "main":    "./devnet.ts",
  "files":   [ "devnet.ts", "devnet-init.mjs" ],
  "bin": { "fadroma-devnet": "devnet.cli.cjs" },
  "description": "Spawn local instance of blockchain for smart contract development.",
  "scripts": {
    "ts:check": "tsc --noEmit",
    "release:dry": "pnpm ts:check && ubik dry",
    "release:wet": "pnpm ts:check && ubik wet --access=public"
  },
  "peerDependencies": {
    "@hackbg/ganesha": "^2.2.5"
  },
  "peerDependenciesMeta": {
    "@hackbg/ganesha": { "optional": true, "description": "For fadroma-devnet command." }
  },
  "dependencies": {
    "@fadroma/client": "workspace:^5",
    "@hackbg/dokeres": "workspace:^4.4.1",
    "@hackbg/portali": "workspace:^1.2.4",
    "@hackbg/kabinet": "workspace:^3.1.2",
    "@hackbg/komandi": "workspace:^5",
    "@hackbg/konfizi": "workspace:*",
    "@hackbg/formati": "workspace:^3.0.1"
  }
}

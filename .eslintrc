{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "es2020": true,
    "mocha": true
  },
  "globals": {
    "process": "writable",
    "globalThis": "writable",
    "describe": "readonly",
    "it": "readonly"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "plugins": [
    "@babel"
  ],
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    }
  },
  "rules": {
    "no-unused-vars": 0,
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "quotes": ["error", "single"],
    "max-len": 0,
    "no-undef": 0,
    "no-duplicate-case": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "brace-style": 0,
    "no-useless-constructor": 0,
    "import/no-extraneous-dependencies": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "no-empty-function": 0,
    "class-methods-use-this": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "no-throw-literal": 0,
    "no-prototype-builtins": 0,
    "import/order": 0,
    "no-restricted-syntax": 0,
    "radix": 0,
    "prefer-destructuring": 0,
    "prefer-promise-reject-errors": 0,
    "prefer-rest-params": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "no-await-in-loop": 0,
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "no-restricted-globals": 0,
    "no-console": 0,
    "import/extensions": 0,
    "prefer-arrow-callback": 0,
    "import/no-cycle": 0,
    "@typescript-eslint/no-empty-function": 0,
    "no-irregular-whitespace": 0,
    "@typescript-eslint/ban-ts-comment": 0,
    "import/no-named-as-default-member": 0,
    "import/no-named-as-default": 0,
    "no-constant-condition": 0,
    "camelcase": 0
  }
}
